# -*- mode: shell-script -*-
#!/usr/bin/env bash

PROJECT_NAME="$1"
PROJECT_BASE="$PROJECT_BASE"
PROJECT_ROOT="${PROJECT_BASE}/$PROJECT_NAME"
GIT_BASE="${ORGANIZATION_GITHUB_LOGIN:=$GITHUB_LOGIN}"

[ ! -d $PROJECT_ROOT ] && cd $PROJECT_BASE && git clone "git@github.com:$GIT_BASE/$PROJECT_NAME.git"

cd $PROJECT_ROOT
